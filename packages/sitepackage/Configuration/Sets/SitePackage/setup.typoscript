tt_content.skygate_test = JSON
tt_content.skygate_test {
    fields {
        appearance =< lib.appearance

        colPos = INT
        colPos {
            field = colPos
        }

        content = TEXT
        content {
            dataProcessing {
                10 = nb-content-blocks-json
                10.as = data
            }
        }

        id = INT
        id {
            field = uid
        }

        type = TEXT
        type {
            field = CType
        }
    }
}


